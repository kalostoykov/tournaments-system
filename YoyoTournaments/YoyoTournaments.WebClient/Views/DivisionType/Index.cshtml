@model IEnumerable<YoyoTournaments.WebClient.Models.DivisionTypeGridViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Divisions in all yoyo tournaments</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("More information", "Details", new { id = item.Id }, new { @class = "btn btn-default" })
        </td>
    </tr>
}

</table>
